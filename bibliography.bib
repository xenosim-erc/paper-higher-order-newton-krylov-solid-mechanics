	keywords = {Animals, Heart Transplantation, Heterotopic, Humans, Orthotopic, Transplantation, Transplantation, Heterotopic},
	pages = {201--206},
	file = {Full Text:C\:\\Users\\aoife\\Zotero\\storage\\FS2BNSQX\\Flécher et al. - 2013 - Heterotopic heart transplantation where do we sta.pdf:application/pdf},
}

